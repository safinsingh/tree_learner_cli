{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "outDir": "dist",
      "declaration": true,
      "sourceMap": true,
      "removeComments": false,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "strict": true
    },
    "include": ["*.ts"],
    "exclude": ["main.ts"]
  }
  